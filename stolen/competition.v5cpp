{"mode":"Text","hardwareTarget":"brain","textContent":"#pragma region VEXcode Generated Robot Configuration\n// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\nmotor fl = motor(PORT1, ratio18_1, false);\n\nmotor fr = motor(PORT16, ratio18_1, true);\n\nmotor bl = motor(PORT2, ratio18_1, false);\n\nmotor br = motor(PORT19, ratio18_1, true);\n\ncontroller Controller1 = controller(primary);\ndigital_out work = digital_out(Brain.ThreeWirePort.A);\nmotor inta = motor(PORT3, ratio18_1, true);\n\nmotor flywheel = motor(PORT9, ratio36_1, false);\n\nmotor colorM = motor(PORT5, ratio18_1, false);\n\ncontroller Controller2 = controller(partner);\nmotor stringM = motor(PORT11, ratio6_1, false);\n\n\n\n// generating and setting random seed\nvoid initializeRandomSeed(){\n  int systemTime = Brain.Timer.systemHighResolution();\n  double batteryCurrent = Brain.Battery.current();\n  double batteryVoltage = Brain.Battery.voltage(voltageUnits::mV);\n\n  // Combine these values into a single integer\n  int seed = int(batteryVoltage + batteryCurrent * 100) + systemTime;\n\n  // Set the seed\n  srand(seed);\n}\n\n\n\nvoid vexcodeInit() {\n\n  //Initializing random seed.\n  initializeRandomSeed(); \n}\n\n\n// Helper to make playing sounds from the V5 in VEXcode easier and\n// keeps the code cleaner by making it clear what is happening.\nvoid playVexcodeSound(const char *soundName) {\n  printf(\"VEXPlaySound:%s\\n\", soundName);\n  wait(5, msec);\n}\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n#pragma endregion VEXcode Generated Robot Configuration\n\n/*----------------------------------------------------------------------------*/\n/*                                                                            */\n/*    Module:       wtf is this                                               */\n/*    Author:       me                                                        */\n/*    Created:      today                                                     */\n/*    Description:  robot go zoom                                             */\n/*                                                                            */\n/*----------------------------------------------------------------------------*/\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint n = 10; // for string motor holding speed, method on line 131\nbool isBlue = true; //im blue ba da bee\nbool sauto = false;\nbool isAustin = true; //I love this line\n\nvoid crabF() {\n  fl.setVelocity(100, percent);\n  fr.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  br.setVelocity(100, percent);\n}\n\nvoid crabL() {\n  fl.setVelocity(-100, percent);\n  fr.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  br.setVelocity(-100, percent);\n}\n\nvoid crabR() {\n  fl.setVelocity(100, percent);\n  fr.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  br.setVelocity(100, percent);\n}\n\nvoid crabB() {\n  fl.setVelocity(-100, percent);\n  fr.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  br.setVelocity(-100, percent);\n}\n\nvoid stopAll() {\n  fl.setVelocity(0, percent);\n  fr.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n}\n\n\nvoid intake() {\n  if (inta.velocity(percent) > 0) {\n    inta.setVelocity(0, percent);\n  }\n  else {\n    inta.setVelocity(100, percent);\n  }\n}\n\nvoid startShoot() {\n  work.set(true);\n  wait(0.5, seconds);\n  work.set(false);\n}\n\nvoid endShoot() {\n  work.set(false);\n}\n\nvoid stringShoot() {\n  wait(0.25, seconds);\n  if (Controller2.ButtonL1.pressing() && Controller2.ButtonL2.pressing()) {\n    stringM.setVelocity(100, percent);\n    stringM.spin(reverse);\n    wait(0.25, seconds);\n    stringM.stop();\n  }\n}\n\nvoid fly() {\n  if (flywheel.velocity(percent) > 0) {\n    flywheel.setVelocity(0, percent);\n  }\n  else {\n    flywheel.setVelocity(85, percent);\n  }\n}\n\nvoid crabJoy() {\n  int xPos = Controller1.Axis4.position();\n  int yPos = Controller1.Axis3.position();\n  int setA = yPos + xPos;\n  int setB = yPos - xPos;\n  fl.setVelocity(setA, percent);\n  br.setVelocity(setA, percent);\n  fr.setVelocity(setB, percent);\n  bl.setVelocity(setB, percent);\n}\n\nvoid crabJoyOLD() { //Austin's middle name\n  int xPos = Controller1.Axis4.position();\n  int yPos = Controller1.Axis3.position();\n  int val;\n  if (yPos >= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis4.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(-1 * val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis4.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(-1 * val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos >= xPos && yPos >= -1 * xPos) {\n    int val = Controller1.Axis3.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos <= -1 * xPos) {\n    int val = Controller1.Axis3.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    //Hi Austin's Mom\n    br.setVelocity(val, percent);\n  }\n}\n\nvoid normalJoy() {\n  int xPos = Controller1.Axis1.position();\n  int yPos = Controller1.Axis2.position();\n  int setA = yPos + xPos;\n  int setB = yPos - xPos;\n  fl.setVelocity(setA, percent);\n  bl.setVelocity(setA, percent);\n  fr.setVelocity(setB, percent);\n  br.setVelocity(setB, percent);\n}\nvoid normalJoyOLD() {\n  int xPos = Controller1.Axis1.position();\n  int yPos = Controller1.Axis2.position();\n  int val;\n  if (yPos >= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis1.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(-1 * val, percent);\n  }\n  else if (yPos <= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis1.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(-1 * val, percent);\n  }\n  else if (yPos >= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis2.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis2.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n}\n\nvoid mcUp() {\n  if (colorM.velocity(percent) < 0) {\n    colorM.setVelocity(0, percent);\n  }\n  else {\n    colorM.setVelocity(-1 * 100, percent);\n  }\n}\n\nvoid mcDown() {\n  if (colorM.velocity(percent) > 0) {\n    colorM.setVelocity(0, percent);\n  }\n  else {\n    colorM.setVelocity(100, percent);\n  }\n}\n\nvoid userControl(void) {\n  Brain.Screen.clearScreen();\n  work.set(false);\n  fl.setVelocity(0, percent);\n  fr.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  flywheel.setVelocity(0, percent);\n  inta.setVelocity(0, percent); //Austin is a funny word(Autism)\n  colorM.setVelocity(0, percent);\n  stringM.setVelocity(n, rpm); // starting string motor rpm\n  colorM.spin(forward);\n  flywheel.spin(forward);\n  inta.spin(forward);\n  fl.spin(forward);\n  fr.spin(forward);\n  bl.spin(forward);\n  br.spin(forward);\n  Controller1.ButtonUp.pressed(crabF);\n  Controller1.ButtonLeft.pressed(crabL);\n  Controller1.ButtonRight.pressed(crabR);\n  Controller1.ButtonDown.pressed(crabB);\n  Controller1.ButtonUp.released(stopAll);\n  Controller1.ButtonLeft.released(stopAll);\n  Controller1.ButtonRight.released(stopAll);\n  Controller1.ButtonDown.released(stopAll);\n\n  //Controller1.Axis3.changed(crabJoy);\n  //Controller1.Axis4.changed(crabJoy);\n\n  Controller1.Axis2.changed(normalJoy);\n  Controller1.Axis1.changed(normalJoy);\n\n  Controller2.ButtonA.pressed(fly);\n\n  Controller1.ButtonR1.pressed(startShoot);\n  Controller1.ButtonR1.released(endShoot);\n\n  Controller2.ButtonL1.pressed(stringShoot);\n  Controller2.ButtonL2.pressed(stringShoot);\n\n  Controller2.ButtonY.pressed(intake);\n\n  Controller2.ButtonUp.pressed(mcUp);\n  Controller2.ButtonUp.released(mcUp);\n  Controller2.ButtonDown.pressed(mcDown);\n  Controller2.ButtonDown.released(mcDown);\n  while (true) {\n    wait(20, msec);\n  }\n}\n\n\nvoid autoStop() {\n  fr.setVelocity(0, percent);\n  fl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n}\n\nvoid autoF(double t) {\n  fr.setVelocity(100, percent);\n  fl.setVelocity(100, percent);\n  br.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  wait(t, seconds);\n}\n\nvoid autoB(double t) {\n  fr.setVelocity(-100, percent);\n  fl.setVelocity(-100, percent);\n  br.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  wait(t, seconds);\n}\n\nvoid autoL(double t) {\n  fl.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  fr.setVelocity(100, percent);\n  br.setVelocity(100, percent);\n  wait(t, seconds);\n}\n\nvoid autoR(double t) {\n  fl.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  fr.setVelocity(-100, percent);\n  br.setVelocity(-100, percent);\n  wait(t, seconds);\n}\n\nvoid colorSeq(bool isFull) {\n  //autoB(0.3);\n  //autoStop();\n  if (isBlue) {\n    colorM.setVelocity(-50, percent); // og 50\n  }\n  else { colorM.setVelocity(50, percent); }\n  if ( isFull ) {\n    wait(1.25, seconds);\n  }\n  else { wait(1, seconds); }\n  colorM.setVelocity(0, percent);\n  autoStop();\n}\n\nvoid preAutonomous(void) {\n  Brain.Screen.clearScreen();\n  fr.setVelocity(0, percent);\n  fl.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  colorM.setVelocity(0, percent);\n  flywheel.setVelocity(0, percent); // change to 100 later\n  flywheel.spin(forward);\n  colorM.spin(forward);\n  fr.spin(forward);\n  fl.spin(forward);\n  br.spin(forward);\n  bl.spin(forward);\n  autoF(1.1);\n  autoL(0.4);\n  autoStop();\n  colorSeq(false);\n}\n\nvoid autoShoot(void) {\n  for (int i = 0; i < 3; i++) {\n    work.set(true);\n    wait(0.5, seconds);\n    work.set(false);\n    wait(0.5, seconds);\n  }\n}\n\nvoid autoString(void) {\n  stringM.setVelocity(100, percent);\n  stringM.spin(reverse);\n  wait(0.25, seconds);\n  stringM.stop();\n}\n\nvoid autonomous(void) {\n  /*\n  Brain.Screen.clearScreen();\n  fr.setVelocity(0, percent);\n  fl.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  colorM.setVelocity(0, percent);\n  flywheel.setVelocity(0, percent); // change to 100 later\n  flywheel.spin(forward);\n  colorM.spin(forward);\n  fr.spin(forward);\n  fl.spin(forward);\n  br.spin(forward);\n  bl.spin(forward);\n  autoF(1.1);\n  autoL(0.4);\n  autoStop();\n  colorSeq(false);\n  */\n  Brain.Screen.clearScreen();\n  fr.setVelocity(0, percent);\n  fl.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  colorM.setVelocity(0, percent);\n  flywheel.setVelocity(0, percent); // change to 100 later, no\n  flywheel.spin(forward);\n  colorM.spin(forward);\n  fr.spin(forward);\n  fl.spin(forward);\n  br.spin(forward);\n  bl.spin(forward);\n  fr.setVelocity(-10, percent); //439-446 is what I added =|\n  fl.setVelocity(-10, percent);\n  br.setVelocity(-10, percent);\n  bl.setVelocity(-10, percent);\n  //maybe if reverse drives into the roller and not away from it. I'm not sure\n  colorSeq(true);\n  autoF(0.2);\n  autoR(0.1);\n  autoStop();\n  wait(50, seconds);\n  autoString();\n}\n\nint main() {\n  // create competition instance\n  competition Competition;\n\n  // Set up callbacks for autonomous and driver control periods.\n  Competition.autonomous(autonomous);\n  Competition.drivercontrol(userControl);\n\n  // Run the pre-autonomous function.\n  preAutonomous();\n\n  // Prevent main from exiting with an infinite loop.\n  while (true) {\n    wait(100, msec);\n  }\n}","textLanguage":"cpp","robotConfig":[{"port":[1],"name":"fl","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[16],"name":"fr","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[2],"name":"bl","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[19],"name":"br","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[],"name":"Controller1","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"},"triportSourcePort":22},{"port":[1],"name":"work","customName":true,"deviceType":"DigitalOut","deviceClass":"digital_out","setting":{"id":"partner"},"triportSourcePort":22},{"port":[3],"name":"inta","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[9],"name":"flywheel","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio36_1"},"triportSourcePort":22},{"port":[5],"name":"colorM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[],"name":"Controller2","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"partner"},"triportSourcePort":22},{"port":[11],"name":"stringM","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio6_1"},"triportSourcePort":22}],"slot":2,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.7","minVersion":"3.1.0","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false,"target":"Physical"}