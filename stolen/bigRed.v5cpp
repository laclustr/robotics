{"mode":"Text","textContent":"#pragma region VEXcode Generated Robot Configuration\n// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\nmotor fl = motor(PORT1, ratio18_1, false);\n\nmotor fr = motor(PORT16, ratio18_1, true);\n\nmotor bl = motor(PORT2, ratio18_1, false);\n\nmotor br = motor(PORT19, ratio18_1, true);\n\ncontroller Controller1 = controller(primary);\ndigital_out work = digital_out(Brain.ThreeWirePort.A);\nmotor in1 = motor(PORT17, ratio18_1, true);\n\nmotor in2 = motor(PORT18, ratio18_1, true);\n\ndigital_out stringP = digital_out(Brain.ThreeWirePort.H);\nmotor flywheel = motor(PORT20, ratio36_1, false);\n\noptical colorIn = optical(PORT9);\nmotor colorM = motor(PORT5, ratio18_1, false);\n\ncontroller Controller2 = controller(partner);\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n#pragma endregion VEXcode Generated Robot Configuration\n\n/*----------------------------------------------------------------------------*/\n/*                                                                            */\n/*    Module:       main.cpp                                                  */\n/*    Author:       {author}                                                  */\n/*    Created:      {date}                                                    */\n/*    Description:  V5 project                                                */\n/*                                                                            */\n/*----------------------------------------------------------------------------*/\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nbool isBlue = false;\n\nvoid crabF() {\n  fl.setVelocity(100, percent);\n  fr.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  br.setVelocity(100, percent);\n}\n\nvoid crabL() {\n  fl.setVelocity(-100, percent);\n  fr.setVelocity(100, percent);\n  bl.setVelocity(100, percent);\n  br.setVelocity(-100, percent);\n}\n\nvoid crabR() {\n  fl.setVelocity(100, percent);\n  fr.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  br.setVelocity(100, percent);\n}\n\nvoid crabB() {\n  fl.setVelocity(-100, percent);\n  fr.setVelocity(-100, percent);\n  bl.setVelocity(-100, percent);\n  br.setVelocity(-100, percent);\n}\n\nvoid stopAll() {\n  fl.setVelocity(0, percent);\n  fr.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n}\n\n\nvoid intake() {\n  if (in1.velocity(percent) > 0) {\n    in1.setVelocity(0, percent);\n    in2.setVelocity(0, percent);\n  }\n  else {\n    in1.setVelocity(100, percent);\n    in2.setVelocity(100, percent);\n  }\n}\n\nvoid intakeBack() {\n  if (in1.velocity(percent) < 0) {\n    in1.setVelocity(0, percent);\n    in2.setVelocity(0, percent);\n  }\n  else {\n    in1.setVelocity(-1 * 100, percent);\n    in2.setVelocity(-1 * 100, percent);\n  }\n}\n\nvoid startShoot() {\n  work.set(true);\n}\n\nvoid endShoot() {\n  work.set(false);\n}\n\nvoid stringShoot() {\n  wait(0.25, seconds);\n  if (Controller2.ButtonL1.pressing() && Controller2.ButtonL2.pressing()) {\n    stringP.set(true);\n  }\n  wait(0.25, seconds);\n  stringP.set(false);\n}\n\nvoid fly() {\n  if (flywheel.velocity(percent) > 0) {\n    flywheel.setVelocity(0, percent);\n  }\n  else {\n    flywheel.setVelocity(60, percent);\n  }\n}\n\nvoid crabJoy() {\n  int xPos = Controller1.Axis4.position();\n  int yPos = Controller1.Axis3.position();\n  int val;\n  if (yPos >= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis4.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(-1 * val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis4.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(-1 * val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos >= xPos && yPos >= -1 * xPos) {\n    int val = Controller1.Axis3.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos <= -1 * xPos) {\n    int val = Controller1.Axis3.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n}\n\nvoid normalJoy() {\n  int xPos = Controller1.Axis1.position();\n  int yPos = Controller1.Axis2.position();\n  int val;\n  if (yPos >= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis1.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(-1 * val, percent);\n  }\n  else if (yPos <= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis1.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(-1 * val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(-1 * val, percent);\n  }\n  else if (yPos >= xPos && yPos >= -1 * xPos) {\n    val = Controller1.Axis2.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n  else if (yPos <= xPos && yPos <= -1 * xPos) {\n    val = Controller1.Axis2.position();\n    fl.setVelocity(val, percent);\n    fr.setVelocity(val, percent);\n    bl.setVelocity(val, percent);\n    br.setVelocity(val, percent);\n  }\n}\n\nvoid colorWheel() {\n  int min, max;\n  if (isBlue) {\n    min = 0;\n    max = 50;\n  }\n  else {\n    min = 50;\n    max = 360;\n  }\n  while ( Controller2.ButtonX.pressing() ) {\n    int x = colorIn.hue();\n    if (x >= min && x <= max) {\n      colorM.setVelocity(100, percent);\n    }\n    else {\n      colorM.setVelocity(0, percent);\n    }\n  }\n}\n\nvoid flyBack() {\n  if (flywheel.velocity(percent) > 0) {\n    flywheel.setVelocity(0, percent);\n  }\n  else {\n    flywheel.setVelocity(-1 * 30, percent);\n  }\n}\n\nvoid mcUp() {\n  if (colorM.velocity(percent) < 0) {\n    colorM.setVelocity(0, percent);\n  }\n  else {\n    colorM.setVelocity(-1 * 10, percent);\n  }\n}\n\nvoid mcDown() {\n  if (colorM.velocity(percent) > 0) {\n    colorM.setVelocity(0, percent);\n  }\n  else {\n    colorM.setVelocity(10, percent);\n  }\n}\n\nvoid preAutonomous(void) {\n  // actions to do when the program starts\n  Brain.Screen.clearScreen();\n  Brain.Screen.print(\"pre auton code\");\n  wait(1, seconds);\n}\n\nvoid autoCol() {\n  colorM.setVelocity(0, percent);\n  colorM.spin(forward);\n  int min, max;\n  if (isBlue) {\n    min = 0;\n    max = 50;\n  }\n  else {\n    min = 50;\n    max = 360;\n  }\n  int x;\n  do {\n    x = colorIn.hue();\n    if (x >= min && x <= max) {\n      colorM.setVelocity(10, percent);\n    }\n    else {\n      colorM.setVelocity(0, percent);\n    }\n  } while( colorIn.hue() >= min && colorIn.hue() <= max);\n}\n\nvoid autoShoot() {\n  work.set(true);\n  wait(1, seconds);\n  work.set(false);\n  wait(1, seconds);\n  work.set(true);\n  wait(1, seconds);\n  work.set(false);\n  wait(1, seconds);\n  flywheel.setVelocity(0, percent);\n  in1.setVelocity(0, percent);\n  in2.setVelocity(0, percent);\n}\n\nvoid autonomous(void) {\n  Brain.Screen.clearScreen();\n  Brain.Screen.print(\"autonomous code\");\n  flywheel.setVelocity(71, percent);\n  in1.setVelocity(100, percent);\n  in2.setVelocity(100, percent);\n  flywheel.spin(forward);\n  in1.spin(forward);\n  in2.spin(forward);\n  fl.setVelocity(50, percent);\n  fr.setVelocity(50, percent);\n  bl.setVelocity(50, percent);\n  br.setVelocity(50, percent);\n  /* OPTIONAL\n  fl.spin(reverse);\n  fr.spin(reverse);\n  bl.spin(reverse);\n  br.spin(reverse);\n  wait(1.1, seconds);\n  */\n  fl.spin(reverse);\n  fr.spin(forward);\n  bl.spin(forward);\n  br.spin(reverse);\n  wait(0.5, seconds);\n  fl.stop();\n  fr.stop();\n  bl.stop();\n  br.stop();\n  autoCol();\n  fl.spin(forward);\n  fr.spin(reverse);\n  bl.spin(reverse);\n  br.spin(forward);\n  wait(0.5, seconds);\n  fl.stop();\n  fr.stop();\n  bl.stop();\n  br.stop();\n  wait(0.3, seconds);\n  fl.spin(forward);\n  fr.spin(reverse);\n  bl.spin(forward);\n  br.spin(reverse);\n  wait(0.85,seconds);\n  fl.stop();\n  fr.stop();\n  bl.stop();\n  br.stop();\n  autoShoot();\n}\n\nvoid userControl(void) {\n  Brain.Screen.clearScreen();\n  Brain.Screen.print(\"Blue\");\n  work.set(false);\n  stringP.set(false);\n  // fl - 1, fr - 16, bl - 2, br - 19\n  fl.setVelocity(0, percent);\n  fr.setVelocity(0, percent);\n  bl.setVelocity(0, percent);\n  br.setVelocity(0, percent);\n  flywheel.setVelocity(0, percent);\n  in1.setVelocity(0, percent);\n  in2.setVelocity(0, percent);\n  colorM.setVelocity(0, percent);\n  colorM.spin(forward);\n  flywheel.spin(forward);\n  in1.spin(forward);\n  in2.spin(forward);\n  fl.spin(forward);\n  fr.spin(forward);\n  bl.spin(forward);\n  br.spin(forward);\n  Controller1.ButtonUp.pressed(crabF);\n  Controller1.ButtonLeft.pressed(crabL);\n  Controller1.ButtonRight.pressed(crabR);\n  Controller1.ButtonDown.pressed(crabB);\n  Controller1.ButtonUp.released(stopAll);\n  Controller1.ButtonLeft.released(stopAll);\n  Controller1.ButtonRight.released(stopAll);\n  Controller1.ButtonDown.released(stopAll);\n\n  Controller1.Axis3.changed(crabJoy);\n  Controller1.Axis4.changed(crabJoy);\n\n  Controller1.Axis2.changed(normalJoy);\n  Controller1.Axis1.changed(normalJoy);\n\n  Controller2.ButtonA.pressed(fly);\n\n  Controller1.ButtonR1.pressed(startShoot);\n  Controller1.ButtonR1.released(endShoot);\n\n  Controller2.ButtonL1.pressed(stringShoot);\n  Controller2.ButtonL2.pressed(stringShoot);\n\n  Controller2.ButtonY.pressed(intake);\n  Controller2.ButtonR2.pressed(intakeBack);\n\n  Controller2.ButtonX.pressed(colorWheel);\n\n  Controller2.ButtonB.pressed(flyBack);\n  Controller2.ButtonB.released(flyBack);\n\n  Controller2.ButtonUp.pressed(mcUp);\n  Controller2.ButtonUp.released(mcUp);\n  Controller2.ButtonDown.pressed(mcDown);\n  Controller2.ButtonDown.released(mcDown);\n  while (true) {\n    wait(20, msec);\n  }\n}\n\nint main() {\n  // create competition instance\n\n  competition Competition;\n\n  // Set up callbacks for autonomous and driver control periods.\n  Competition.autonomous(autonomous);\n  Competition.drivercontrol(userControl);\n\n  // Run the pre-autonomous function.\n  preAutonomous();\n\n  // Prevent main from exiting with an infinite loop.\n  while (true) {\n    wait(100, msec);\n  }\n}","textLanguage":"cpp","rconfig":[],"slot":0,"platform":"V5","sdkVersion":"20220726.10.00.00","appVersion":"2.4.0","minVersion":"2.4.0","fileFormat":"1.2.0","icon":"","targetBrainGen":"First","target":"Physical"}